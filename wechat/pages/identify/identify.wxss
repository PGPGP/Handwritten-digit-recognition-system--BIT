/* pages/identify/identify.wxss */
@import "/images/iconfont1.wxss";
.bg{
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%; /*宽度 高度*/
  /*z-index 较大的元素会覆盖较小的元素在上层进行显示*/
  z-index: -1;
}
.feedback {

  width:100rpx;
  height:60rpx;
  text-align:center;
  line-height:38rpx;
  background-color:transparent;
  border:none;
  font-size: 30rpx;
  box-sizing: border-box;
  left:2%;
  top:90%;
  color: rgb(100, 184, 240)
}


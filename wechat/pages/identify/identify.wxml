<!--pages/identify/identify.wxml-->
<image class="bg" src="/images/星空.jpg"></image>

<view  style="height: 350rpx; width: 750rpx">
  <text style="position: relative; left: 0%; top: 5%; color: white">检测识别图像：</text>
  <image selectable="true" style="height: 350rpx; width: 500rpx; position: relative; margin-left: 32%; margin-top: -5%" src="{{identifyImage}}" />
 
</view>

<view  style="height: 350rpx; width: 750rpx">
  <text style="position: relative; left: 0%; top: 40%;color:white ">识别结果为：</text>
 
  <text selectable style="position: relative; top: 40%;color:white">{{text}}</text>
</view>

<view style="height: 150rpx; width: 750rpx">
<button class='feedback' bind:tap="feedback">反馈结果
</button>
</view>

<view class="iconfont icon-guanbi" style="position: relative; margin-left: 49%; margin-top: 20%; width: 54rpx; height: 50rpx; display: flex; box-sizing: border-box; color: aliceblue" bindtap="next_calculator"></view>
